#include <stdio.h>

int main() {
    int blockSize[10], processSize[10], blockCount, processCount;
    int allocation[10], i, j;

    printf("Enter number of blocks: ");
    scanf("%d", &blockCount);

    printf("Enter size of each block:\n");
    for(i = 0; i < blockCount; i++)
        scanf("%d", &blockSize[i]);

    printf("Enter number of processes: ");
    scanf("%d", &processCount);

    printf("Enter size of each process:\n");
    for(i = 0; i < processCount; i++)
        scanf("%d", &processSize[i]);

    for(i = 0; i < processCount; i++)
        allocation[i] = -1;

    for(i = 0; i < processCount; i++) {
        for(j = 0; j < blockCount; j++) {
            if(blockSize[j] >= processSize[i]) {
                allocation[i] = j;
                blockSize[j] -= processSize[i];
                break;
            }
        }
    }

    printf("\nProcess No.\tSize\tBlock No.\n");
    for(i = 0; i < processCount; i++) {
        if(allocation[i] != -1)
            printf("%d\t\t%d\t%d\n", i+1, processSize[i], allocation[i]+1);
        else
            printf("%d\t\t%d\tNot Allocated\n", i+1, processSize[i]);
    }

    return 0;
}
